<% @page_title = "Create Drone" %>

<%= link_to("<< Back to Index", {:action => 'index'}, :class => 'back-link') %>

<div class="drone new">
	<h2>Create Drone</h2>

	<%= form_for(:drone, :url => {:action => 'create'}) do |f| %>
		
		<%= render(:partial => "form", :locals => {:f => f}) %>

		<div class="form-buttons">
			<%= submit_tag("Create Drone") %>
		</div>
	<% end %>

</div>